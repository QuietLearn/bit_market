<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.huobi.dao.SymbolsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.huobi.model.Symbols">
        <result column="base_currency" property="baseCurrency" />
        <result column="quote_currency" property="quoteCurrency" />
        <result column="price_precision" property="pricePrecision" />
        <result column="amount_precision" property="amountPrecision" />
        <result column="symbol_partition" property="symbolPartition" />
